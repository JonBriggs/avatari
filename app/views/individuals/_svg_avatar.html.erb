<svg
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:cc="http://creativecommons.org/ns#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:svg="http://www.w3.org/2000/svg"
   xmlns="http://www.w3.org/2000/svg"
   version="1.1"
   width="150"
   height="150"
   id="svg2985"
   xml:space="preserve"><metadata
     id="metadata2991"><rdf:RDF><cc:Work
         rdf:about=""><dc:format>image/svg+xml</dc:format><dc:type
           rdf:resource="http://purl.org/dc/dcmitype/StillImage" /><dc:title></dc:title></cc:Work></rdf:RDF></metadata><defs
     id="defs2989"><clipPath
       id="clipPath2999"><path
         d="M 0,792 612,792 612,0 0,0 0,792 z"
         id="path3001" /></clipPath></defs>
  <%= render @individual.avatar if @individual.avatar %>
  <% @individual.worn_items.each do |witem| %>
    <%= render witem.item %>
  <% end %>

</svg>
<script>
  <%@individual.worn_items.each do |worn_item|%>
    <% if worn_item.color? %>
      <%color_code, action, color = worn_item.color.split(":")%>
      d3.selectAll("#g_<%=worn_item.item.target_area.name.downcase%> path.<%=color_code%>").transition().duration(2000).style("<%=action%>", "<%=color%>");
    <%end%>
  <%end%>
</script>